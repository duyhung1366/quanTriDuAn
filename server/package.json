{
  "name": "ksteam-web-server",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://bitbucket.org/koolsoft-dev-repos/ksteam",
  "author": "duypv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty index.ts",
    "pre-build": "grep -rl '\"outDir\": \"./dist\"' tsconfig.json | xargs sed -i 's#\"outDir\": \"./dist\"#\"outDir\": \"./build\"#g'",
    "pre-build-mac": "grep -rl '\"outDir\": \"./dist\"' tsconfig.json | xargs sed -i '' 's#\"outDir\": \"./dist\"#\"outDir\": \"./build\"#g'",
    "post-build": "grep -rl '\"outDir\": \"./build\"' tsconfig.json | xargs sed -i 's#\"outDir\": \"./build\"#\"outDir\": \"./dist\"#g'",
    "post-build-mac": "grep -rl '\"outDir\": \"./build\"' tsconfig.json | xargs sed -i '' 's#\"outDir\": \"./build\"#\"outDir\": \"./dist\"#g'",
    "build": "run-script-os",
    "build:default": "npm run pre-build && tsc -p . && npm run post-build && rm -rf dist && mv build dist",
    "build:macos": "npm run pre-build-mac && tsc -p . && npm run post-build-mac && rm -rf dist && mv build dist",
    "start": "cross-env NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.4",
    "axios": "^1.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "cross-env": "7.0.3",
    "crypto-js": "^4.1.1",
    "discord.js": "14.8.0",
    "dotenv": "16.0.3",
    "dotenv-expand": "9.0.0",
    "express": "4.18.2",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "momentjs": "^2.0.0",
    "mongoose": "6.1.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.1",
    "run-script-os": "^1.1.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.3"
  }
}
